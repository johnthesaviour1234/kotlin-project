{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "grocery-delivery-api",
  "rootDirectory": "grocery-delivery-api",
  "framework": "nextjs",
  "functions": {
    "grocery-delivery-api/pages/api/**/*.js": {
      "runtime": "nodejs22.x",
      "regions": ["iad1"]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}